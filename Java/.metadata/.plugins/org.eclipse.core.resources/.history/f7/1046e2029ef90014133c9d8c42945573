package Filmator;

import java.util.ArrayList;

public class Filme {
	
	private ArrayList<Ator> elenco = new ArrayList<Ator>();
	
	public void adicionaAtor(Ator umAtor){
		
		elenco.add(umAtor);
	}
	
	public String listaElenco(){
		
		for (Ator ator : elenco) {
			
			return ator.toString(); 
		}		
	}
	
}
