package Filmator;

public class testaFilmator {

	public static void main(String[] args) {
		
		Ator a1 = new Ator("Rodrigo Frohlich");
		Ator a1 = new Ator("Rodrigo teste");
		Ator a1 = new Ator("Rodrigo fera");
		Ator a1 = new Ator("Rodrigo gato");
		Ator a2 = new Ator("Cassiano");
		
		Filme f = new Filme();
		
		f.adicionaAtor(a1);
		f.adicionaAtor(a2);
		
		System.out.println(f.listaElenco());
		System.out.println(f.procuraAtor("Rodrigo"));		
		
	}

}
